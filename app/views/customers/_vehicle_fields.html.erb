<div class="col-md-offset-3">
  <div class="nested-fields">
    <b>Vehicle Detail</b>
    <div class="form-group">
      <%= f.text_field :year, class: 'form-control', placeholder: 'Year' %>
    </div>
    <div class="form-group">
      <%= f.text_field :make, class: 'form-control datepicker', placeholder: 'Make' %>
    </div>
    <div class="form-group">
      <%= f.text_field :model, class: 'form-control', placeholder: 'Model No.' %>
    </div>
    <div class="form-group">
      <%= f.text_field :trim, class: 'form-control', placeholder: 'Trim' %>
    </div>
    <div class="form-group">
      <%= f.text_field :vin, class: 'form-control', placeholder: 'Vin' %>
    </div>
    <div class="form-group">
      <%= f.text_field :style, class: 'form-control', placeholder: 'Style' %>
    </div>
    <div class="form-group">
      <%= f.hidden_field :_destroy %>
      <%= link_to 'remove', '#', class: "remove_fields btn btn-danger" %>
    </div>
  </div>
</div>

<script type="text/javascript">
  $(function() {
    $('.datepicker').datepicker({
      dateFormat: "yy-mm-dd"
    });
  });
</script>